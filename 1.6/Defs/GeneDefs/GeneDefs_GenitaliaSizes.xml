    <displayOrderInCategory>754</displayOrderInCategory>
    <exclusionTags>
      <li>VaginaSize</li>
    </exclusionTags>
    <modExtensions>
			<li Class="RJW_Genes.BoundedExtension">
        <minimum>0.6</minimum>
        <maximum>1.01</maximum>
			</li>
		</modExtensions>
  </GeneDef>

